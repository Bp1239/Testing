<script>
// Define the ID of Ajuna Network
const ajunaId = 'ajuna-network';

// Send a GET request to the CoinGecko API
fetch(`https://api.coingecko.com/api/v3/coins/${ajunaId}/tickers`)
  .then(response => response.json())
  .then(data => {
    // Get the current value of Ajuna Network from the API response
    const ajunaValue = data.tickers[0].last.toFixed(2);
    // Display the value in the "bayunworth" text class
    const bayunworthElement = document.querySelector('.bayunworth');
    bayunworthElement.textContent = `1 Baju = ${ajunaValue} USD`;
  })
  .catch(error => {
    console.error('Error:', error);
  });

</script></body></html>
