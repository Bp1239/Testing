  .then(response => response.json())
  .then(data => {
    const txs = data.ajuna.txs;
    const txsCountElements = document.querySelectorAll('.txs-count');
    txsCountElements.forEach(element => {
        element.innerHTML = txs;
    });
    const nfts = data.ajuna.nfts;
    const nftsCountElements = document.querySelectorAll('.nfts-count');
    nftsCountElements.forEach(element => {
        element.innerHTML = nfts;
    });
    const wallets = data.ajuna.wallets;
    const walletsCountElements = document.querySelectorAll('.wallets-count');
    walletsCountElements.forEach(element => {
        element.innerHTML = wallets;
    });
    const players = data.aaa.players;
    const playerCountElements = document.querySelectorAll('.players-count');
    playerCountElements.forEach(element => {
        element.innerHTML = players;
    });
    const avatars = data.aaa.avatars;
    const avatarsCountElements = document.querySelectorAll('.avatars-count');
    avatarsCountElements.forEach(element => {
        element.innerHTML = avatars;
    });
    const forges = data.aaa.forges;
    const forgesCountElements = document.querySelectorAll('.forges-count');
    forgesCountElements.forEach(element => {
        element.innerHTML = forges;
    });
